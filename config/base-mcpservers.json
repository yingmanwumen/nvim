{
  "mcpServers": {
    "context7": {
      "disabled": true,
      "type": "streamable-http",
      "url": "https://mcp.context7.com/mcp"
    },
    "fetch": {
      "args": ["mcp-server-fetch"],
      "command": "uvx",
      "disabled": false
    },
    "github": {
      "disabled": true,
      "headers": {
        "Authorization": "Bearer ${GITHUB_PERSONAL_API_KEY}"
      },
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "sequentialthinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "autoApprove": ["sequentialthinking"],
      "command": "npx",
      "disabled": false,
      "disabled_tools": []
    },
    "text-editor": {
      "args": ["mcp-text-editor"],
      "autoApprove": ["get_text_file_contents", "edit_text_file_contents"],
      "command": "uvx",
      "custom_instructions": {
        "text": "When editing lines, be careful about '\\n'."
      },
      "disabled": true
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": true,
      "disabled_prompts": [],
      "disabled_resources": ["mcphub://docs", "mcphub://changelog"],
      "disabled_tools": ["get_current_servers", "toggle_mcp_server"]
    },
    "neovim": {
      "autoApprove": ["read_file", "read_multiple_files", "execute_command"],
      "custom_instructions": {
        "disabled": false,
        "text": "When invoking tools, put the specific tool arguments into `tool_inputs`, such as `{..., tool_inputs: {...}}`"
      },
      "disabled": false,
      "disabled_prompts": ["parrot"],
      "disabled_resources": [],
      "disabled_tools": [
        "execute_lua",
        "delete_items",
        "list_directory",
        "find_files",
        "move_item"
      ]
    }
  }
}

